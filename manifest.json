{
  "manifest_version": 2,
  "name": "Jump Kick",
  "version": "1.0",
  "description": "Quickly switch between open tabs with fuzzy search",
  "permissions": ["tabs", "activeTab", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "jump-kick: Quick Tab Switcher"
  },
  "commands": {
    "open-tab-switcher": {
      "suggested_key": {
        "mac": "Command+Shift+Space",
        "default": "Ctrl+Shift+Space"
      },
      "description": "Quick tab switcher"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}